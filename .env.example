# Configuration file for NCBI FASTA Downloader
# This file is created during setup and contains no personal information

# Email for NCBI API (set during setup)
EMAIL=your.email@example.com

# Performance settings
BATCH_SIZE=200
MAX_WORKERS=4
RATE_LIMIT_DELAY=0.34

# File paths (relative to project root)
OUTPUT_DIR=output
INPUT_FILE=ncbi_ids_dict.tsv

# Advanced settings
COMPRESSION=false
DUPLICATE_DETECTION=true
CHECKSUM_VALIDATION=true
LOG_LEVEL=INFO

# NCBI API settings
NCBI_DATABASE=protein
NCBI_RETTYPE=fasta
NCBI_RETMODE=text
